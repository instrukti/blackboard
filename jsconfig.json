{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "ignoreDeprecations": "5.0"
  },
  "paths": {
    "$lib": ["src/lib"],
    "$lib/*": ["src/lib/*"],
    "Base": ["src/baseApp"],
    "Base/*": ["src/baseApp/*"]
  }
}
